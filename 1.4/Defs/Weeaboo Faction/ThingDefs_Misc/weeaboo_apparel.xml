<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<!-- Bandana -->
	<ThingDef ParentName="HatMakeableBase">
		<defName>weebbandana</defName>
		<label>tendrilous hachimaki bandana</label>
		<description>This simple bandana was intended to mirror the Hachimaki, which is a warrior's headband baring the image of the Rising Sun or some sort of inspirational slogan. This "Hachimaki" is tied together with longer strands of its material, giving it based tendrils that wave in the wind when its wearer runs. This effect, coupled with sticking one's arms out, makes the wearer more aerodynamic.</description>
		<soundInteract>TSP_kakegoe_rhythm_sticks</soundInteract>
		<graphicData>
			<texPath>weeaboo/weebbandana</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<uiIconScale>1.25</uiIconScale>
		<costStuffCount>25</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<thingCategories>
			<li>TSP_WeeabooApparel</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<WorkToMake>4000</WorkToMake>
			<Mass>0.007</Mass>
			<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.2</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.5</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>0.8</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.5</MoveSpeed>
		</equippedStatOffsets>
		<generateCommonality Inherit="False">1</generateCommonality>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<developmentalStageFilter>Child, Adult</developmentalStageFilter>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
				<wornGraphicPath>weeaboo/weebbandana</wornGraphicPath>
				<!-- <hatRenderedFrontOfFace>true</hatRenderedFrontOfFace> -->
				<layers>
					<li>Overhead</li>
				</layers>
				<tags>
					<li>WeebClothes</li>
					<li>WeebHat</li>
					<li>WeebGirl</li>
					<li>WeebSpecialHat</li>
					<li>WeebKenshiSamurai</li>

					<!-- Mod Support -->
					<li>HSApparel_RoyaltyClothes</li>
					<li>OniWear</li>
					<li>OniHeadWear</li>
					<li>JeubWear</li>
					<li>ST_BeetClothing</li>
					<li>ST_GnomeClothing</li>
					<li>PoorPirateClothing</li>
					<li>NormalPirateClothing</li>
					<li>Pirate</li>
					<li>Apparel_SamuraiPeasant</li>
					<li>HSApparel_EdoClanPeasant</li>
					<li>HSApparel_EdoClanBase</li>
					<li>samurai</li>
					<li>anarchiste</li>
					<li>Samuraihead</li>
				</tags>
				<defaultOutfitTags>
					<li>Worker</li>
					<li>Soldier</li>
					<li>WeebClothes</li>
				</defaultOutfitTags>
			</apparel>
        <!-- Mod cross-compatibility for custom Caravans -->
      	<tradeTags>
			<li>OniApparel</li>
	    </tradeTags>
		<!--
		<colorGenerator Class="ColorGenerator_Options">
		  <options>
			<li>
