	<!-- Straw hat -->
	<ThingDef ParentName="HatMakeableBase">
		<defName>weebstrawhat</defName>
		<label>Rice Field Hat</label>
		<description>An Oriental-style straw hat with a familiar face on it. WELCOME TO THE RICE FIELDS!</description>
		<graphicData>
			<texPath>weeaboo/weebstrawhat</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<uiIconScale>1.25</uiIconScale>
		<costStuffCount>25</costStuffCount>
		<stuffCategories>
			<li>Woody</li>
			<li>Metallic</li>

			<!-- Mod Support -->
			<li MayRequire="Dubwise.Rimefeller">Plastic</li>
			<li MayRequire="Dubwise.Rimefeller">Composite</li>
			<li MayRequire="sarg.alphabiomes">AB_ExoticMetals</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<WorkToMake>7000</WorkToMake>
			<Mass>0.07</Mass>
			<StuffEffectMultiplierArmor>0.4</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>1.5</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>0.8</EquipDelay>
		</statBases>
		<thingCategories>
			<li>TSP_WeeabooApparel</li>
		</thingCategories>
		<generateCommonality>0.4</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<wornGraphicPath>weeaboo/weebstrawhat</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>WeebClothes</li>
				<li>WeebHat</li>
				<li>WeebSpecialHat</li>

				<!-- Mod Support -->
				<li>HSApparel_RoyaltyClothes</li>
				<!--
				<li>Apparel_SamuraiPeasant</li>
				<li>Apparel_SamuraiStraw</li>
				<li>HSApparel_EdoClanPeasant</li>
				<li>OniWear</li>
				<li>OniHeadWear</li>
				<li>TW_Qin_PingMin</li>
				<li>TW_Tang_PingMin</li>
				-->
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
				<li>WeebClothes</li>
			</defaultOutfitTags>
		</apparel>
		
		<!-- Mod cross-compatibility for custom Caravans -->
		<tradeTags>
			<li>OniApparel</li>
		</tradeTags>
		<!--
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(0.33,0.33,0.33,1)</only>
				</li>
				<li>
					<weight>10</weight>
