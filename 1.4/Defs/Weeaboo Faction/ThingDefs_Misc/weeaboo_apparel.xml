	<!-- Wig -->
	<ThingDef ParentName="HatMakeableBase">
		<defName>weebwig</defName>
		<label>Anime wig</label>
		<description>A colourful wig. It is cheap and it smells like plastic, but it provides obscene resistance to the cold if it is made of textiles. In fact, the cold resistance might make it worth humiliating oneself by wearing it.</description>
		<soundInteract>TSP_freedomtoons_waifu2</soundInteract>
		<graphicData>
			<texPath>weeaboo/weebwig</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<uiIconScale>1.25</uiIconScale>
		<costStuffCount>25</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
			<li>Metallic</li>

			<!-- Mod Support -->
			<li MayRequire="Dubwise.Rimefeller">Plastic</li>
			<li MayRequire="Dubwise.Rimefeller">Composite</li>
			<li MayRequire="sarg.alphabiomes">AB_Exotic</li>
			<li MayRequire="sarg.alphabiomes">AB_ExoticMetals</li>
		</stuffCategories>
		<thingCategories>
			<li>TSP_WeeabooApparel</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<WorkToMake>7000</WorkToMake>
			<Mass>0.66</Mass>
			<StuffEffectMultiplierArmor>0.85</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>1.5</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>-0.05</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>0.8</EquipDelay>
		</statBases>
		<generateCommonality>0.5</generateCommonality>
		<apparel>
			<gender>Male</gender>
			<countsAsClothingForNudity>true</countsAsClothingForNudity>
			<developmentalStageFilter>Child, Adult</developmentalStageFilter>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<wornGraphicPath>weeaboo/weebwig</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>WeebClothes</li>
				<li>WeebElite</li>
				<li>WeebKenshi</li>

				<!-- Mod Support -->
				<li>HSApparel_RoyaltyClothes</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
				<li>WeebClothes</li>
			</defaultOutfitTags>
		</apparel>
		<!-- Mod cross-compatibility for custom Caravans -->
		<tradeTags>
			<li>OniApparel</li>
		</tradeTags>
