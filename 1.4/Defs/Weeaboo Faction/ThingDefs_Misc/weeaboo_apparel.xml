	<!-- weebshirt -->
	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_WeebShirt</defName>
		<label>Weeaboo Shirt</label>
		<description>A simple T-shirt with the wearer's Waifu- the DEFINITELY NOT FICTIONAL object of his fantasies and romantic obsessions- printed on it.</description>
		<soundInteract>TSP_freedomtoons_waifu1</soundInteract>
		<graphicData>
			<texPath>weeaboo/weebshirt/weebshirt</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<costStuffCount>55</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<thingCategories>
			<li>TSP_WeeabooApparel</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>2000</WorkToMake>
			<Mass>0.25</Mass>
			<StuffEffectMultiplierArmor>0.65</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.7</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.6</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<apparel>
			<!-- <gender>Male</gender> -->
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
			</bodyPartGroups>
			<wornGraphicPath>weeaboo/weebshirt/weebshirt</wornGraphicPath>
			<layers>
				<li>OnSkin</li>
			</layers>
			<tags>
				<li>WeebClothes</li>
				<li>WeebGirl</li>
				<li>WeebSpecialShirt</li>

				<!-- Mod Support -->
				<li>HSApparel_RoyaltyClothes</li>
				<li>HSApparel_RoyaltyClothes_Hakama</li>
				<li>OniWear</li>
				<li>Samuraihead</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
				<li>Soldier</li>
				<li>WeebClothes</li>
				<li>Slave</li>
			</defaultOutfitTags>
		</apparel>
		<generateCommonality>1</generateCommonality>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
		<comps>
			<li Class="CompProperties_Biocodable"/>
		</comps>
	</ThingDef>

