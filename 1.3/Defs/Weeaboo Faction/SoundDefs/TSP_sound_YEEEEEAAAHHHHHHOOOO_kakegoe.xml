<?xml version="1.0" encoding="utf-8"?>
<Defs>	<!-- These sound effects are hundreds of years old and are in the Public Domain. -->	<!-- https://www.youtube.com/watch?v=VKMw2it8dQY -->
  <SoundDef>
    <defName>TSP_kakegoe_scream_full</defName>
    <eventNames />
    <maxSimultaneous>1</maxSimultaneous>
    <subSounds>
      <li>
        <onCamera>True</onCamera>
    				<grains>
              <li Class="AudioGrain_Folder">
                <clipFolderPath>TSP_YEEEAAHHHHHOOOOOOH_kakegoe</clipFolderPath>
              </li>
            </grains>
            <volumeRange>
              <min>6</min>
              <max>8</max>
           </volumeRange>
           <pitchRange>
             <min>1</min>
             <max>1</max>
           </pitchRange>
      </li>
    </subSounds>
  </SoundDef>
