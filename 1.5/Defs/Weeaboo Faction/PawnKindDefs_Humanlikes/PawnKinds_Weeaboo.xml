		<maxGenerationAge>30</maxGenerationAge>
		<apparelAllowHeadgearChance>1</apparelAllowHeadgearChance>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<!--
		<royalTitleChance MayRequire="Heis.Edo.Themed.Expansion">0.2</royalTitleChance>
		<titleSelectOne MayRequire="Heis.Edo.Themed.Expansion">
			<li MayRequire="Heis.Edo.Themed.Expansion">HSAshigaru</li>
			<li MayRequire="Heis.Edo.Themed.Expansion">HSRonin</l>
		</titleSelectOne>
		-->
		<!-- <apparelRequired> -->
		<!-- If you have the Mod -->
		<!-- <li MayRequire="Trickity.Samurai.Faction">TSFApparel_SashimonoBanner</li> -->
		<!-- <li MayRequire="Heis.Edo.Themed.Expansion">HSApparel_Hakama</li> -->
		<!-- </apparelRequired> -->
		<apparelTags>
			<!--
			<li>WeebClothes</li>
			-->
			<li>WeebHat</li>
			<li>WeebPants</li>
			<li>WeebLastResortTag</li>

			<!-- If you have the Mod -->
			<li>Apparel_SamuraiUtility</li>
			<li>Apparel_NinjaSmoke</li>
			<li>zori</li> <!-- Zori Sandals -->
			<li>Apparel_SamuraiStraw</li> <!-- Provides clothing for cold weather -->
			<li>Hakama</li>
		</apparelTags>
		<forcedTraits>
			<li MayRequire="Heis.Edo.Themed.Expansion">HSRonin</li>
			<li MayRequire="VanillaExpanded.VanillaTraitsExpanded">VTE_Insatiable</li>
			<li>Undergrounder</li>
			<li>DislikesWomen</li>
		</forcedTraits>
