<?xml version="1.0" encoding="utf-8"?><!-- If you have the Qux Samurai Mod, this will prevent the Pawns incorporated into the Weeaboo Faction from spawning naked, because that Mod has requirements for another Mod that this one does not. As a bonus, it doubles as an Easter Egg. --><Patch>	<!-- Aelanna's Model No-Match Detector and Insertion Code -->	<!-- Special thanks to Aelanna for straight-up saving me scores of headaches! -->	<!-- Add comps field to Settlement WorldObjectDef if it doesn't exist -->	<Operation Class="PatchOperationConditional">		<xpath>/Defs/ThingDef[defName="Apparel_Robe"]/apparel/tags</xpath>		<nomatch Class="PatchOperationAdd">			<xpath>/Defs/ThingDef[defName="Apparel_Robe"]/apparel</xpath>			<success>Always</success>			<value>				<tags></tags>			</value>		</nomatch>	</Operation>		<!-- Add comps field to Settlement WorldObjectDef if it doesn't exist -->	<Operation Class="PatchOperationConditional">		<xpath>/Defs/PawnKindDef[defName="Samurai_artilleur"]/apparelTags</xpath>		<nomatch Class="PatchOperationAdd">			<xpath>/Defs/PawnKindDef[defName="Samurai_artilleur"]</xpath>			<success>Always</success>			<value>				<apparelTags></apparelTags>			</value>		</nomatch>	</Operation>	<Operation Class="PatchOperationConditional">	<!-- DO THE GENTILES KNOW? -->		<xpath>/Defs/PawnKindDef[defName="Samuraipaysan"]/apparelRequired</xpath>		<!-- If they do, SHUT IT DOWN. -->		<match Class="PatchOperationRemove">			<success>Always</success>			<xpath>/Defs/PawnKindDef[defName="Samuraipaysan"]/apparelRequired</xpath>		</match>		<!-- If the first XPATH does not exist, the original code stands. All according to plan. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/PawnKindDef[defName="Samuraipaysan"]</xpath>		<!-- If it does, add this. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/PawnKindDef[defName="Samuraipaysan"]/apparelTags</xpath>			<value>				<li>WeebClothes</li>				<li>HSApparel_EdoClanPeasant</li>				<li>HSApparel_EdoClanBase</li>				<li>Apparel_SamuraiBasic</li>				<li>Apparel_SamuraiPeasant</li>				<li>Hakama</li>				<li>Medieval</li>				<li>MedievalApparel</li>				<li>MedievalApparelBasic</li>				<li>zori</li>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- DO THE GENTILES KNOW? -->		<xpath>/Defs/PawnKindDef[defName="Samuraicivil"]/apparelRequired</xpath>		<!-- If they do, SHUT IT DOWN. -->		<match Class="PatchOperationRemove">			<success>Always</success>			<xpath>/Defs/PawnKindDef[defName="Samuraicivil"]/apparelRequired</xpath>		</match>		<!-- If the first XPATH does not exist, the original code stands. All according to plan. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/PawnKindDef[defName="Samuraicivil"]</xpath>		<!-- If it does, add this. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/PawnKindDef[defName="Samuraicivil"]/apparelTags</xpath>			<value>				<li>WeebClothes</li>				<li>HSApparel_EdoClanPeasant</li>				<li>HSApparel_EdoClanBase</li>				<li>Apparel_SamuraiBasic</li>				<li>Apparel_SamuraiPeasant</li>				<li>Hakama</li>				<li>zori</li>				<li>Medieval</li>				<li>MedievalApparel</li>				<li>MedievalApparelBasic</li>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/PawnKindDef[defName="Samurai_artilleur"]/apparelRequired</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationReplace">			<success>Always</success>			<xpath>/Defs/PawnKindDef[defName="Samurai_artilleur"]/apparelRequired</xpath>			<value>				<apparelRequired Inherit="False">					<li>samurai_tete_artilleur</li>				</apparelRequired>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/PawnKindDef[defName="Samurai_artilleur"]/apparelTags</xpath>		<!-- If it does, add this. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/PawnKindDef[defName="Samurai_artilleur"]/apparelTags</xpath>			<value>				<li>WeebClothes</li>				<li>Hakama</li>				<li>zori</li>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation>		<!-- Aelanna's Model No-Match Detector and Insertion Code -->	<!-- Special thanks to Aelanna for straight-up saving me scores of headaches! -->	<!-- Add comps field to Settlement WorldObjectDef if it doesn't exist -->	<Operation Class="PatchOperationConditional">		<xpath>/Defs/ThingDef[defName="Apparel_Robe"]/apparel/tags</xpath>		<nomatch Class="PatchOperationAdd">			<xpath>/Defs/ThingDef[defName="Apparel_Robe"]/apparel</xpath>			<success>Always</success>			<value>				<tags></tags>			</value>		</nomatch>	</Operation>		<Operation Class="PatchOperationConditional">	<!-- Does this exist? -->		<xpath>/Defs/ThingDef[defName="Apparel_Robe"]/apparel/tags</xpath>		<!-- If it does, add this. -->		<match Class="PatchOperationAdd">			<success>Always</success>			<xpath>/Defs/ThingDef[defName="Apparel_Robe"]/apparel/tags</xpath>			<value>				<li>Robe</li>				<li>Medieval</li>				<li>MedievalApparel</li>				<li>MedievalApparelDecent</li>				<li>ST_GnomeClothing</li>				<li>JeubWear</li>			</value>		</match>		<!-- In the end, something will always get added. -->	</Operation></Patch>