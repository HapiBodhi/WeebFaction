<?xml version="1.0" encoding="utf-8" ?><!-- MILK BORPA MILK BORPA MILK --><Patch>	<Operation Class="PatchOperationConditional">		<!-- Does this exist? -->		<xpath>/Defs/ThingDef[defName="BORPA_Milk"]</xpath>		<!-- If it does, do this replacement. -->		<match Class="PatchOperationReplace">			<success>Always</success>			<xpath>/Defs/PawnKindDef[@Name="WeeabooBase"]/invFoodDef</xpath>			<value>				<invFoodDef>BORPA_Milk</invFoodDef>			</value>		</match>		<!-- If the first XPATH does not exist, the original code stands. -->	</Operation></Patch>